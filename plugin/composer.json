{
    "require": {
        "php": "^8.1",
        "illuminate/collections": "^10.49",
        "nesbot/carbon": "^2.54.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "CurtainCall\\": ["CurtainCall/"]
        },
        "files": [
            "functions.php"
        ]
    },
    "autoload-dev": {
        "files": [
            "functions-dev.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
