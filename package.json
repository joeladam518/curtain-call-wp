{
    "name": "curtaincallwp",
    "description": "Custom Post types for productions, casts and crews",
    "version": "0.7.0",
    "type": "module",
    "license": "MIT",
    "scripts": {
        "dev": "rollup -c --watch",
        "build": "rollup -c",
        "typecheck": "npm run typecheck:node && npm run typecheck:web",
