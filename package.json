        "@types/wordpress__plugins": "^3.0.3",
        "@types/wordpress__rich-text": "^6.4.5",
        "@wordpress/scripts": "^31.1.0",
        "autoprefixer": "^10.4.20",
        "fs-extra": "^11.2.0",
        "moment": "^2.29.2",
        "pikaday": "^1.8.0",
        "postcss": "^8.4.49",
